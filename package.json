{
  "name": "php_codesniffer",
  "version": "1.2.7",
  "private": true,
  "description": "GitHub PHP_CodeSniffer action. This workflow check the PHP, CSS and Javascript files for the wanted coding standards.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novusvetus/action-php_codesniffer.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Novusvetus",
  "license": "BSD-3",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "5.0.3",
    "@octokit/webhooks": "10.1.3",
    "picomatch": "2.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.9",
    "@types/picomatch": "2.3.0",
    "@typescript-eslint/parser": "^5.33.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.22.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  }
}
