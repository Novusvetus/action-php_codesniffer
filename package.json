{
  "name": "php_codesniffer",
  "version": "1.2.0",
  "private": true,
  "description": "GitHub PHP_CodeSniffer action. This workflow check the PHP, CSS and Javascript files for the wanted coding standards.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novusvetus/action-php_codesniffer.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Novusvetus",
  "license": "BSD-3",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "5.0.0",
    "@octokit/webhooks": "9.20.0",
    "picomatch": "2.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.13",
    "@types/picomatch": "2.3.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.4.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.4"
  }
}
